{
  "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["react", "prettier", "markdown"],
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  },
  "env": {
    "es2017": true,
    "node": true,
    "browser": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
